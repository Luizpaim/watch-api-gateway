{
  "name": "watch-api-gateway",
  "version": "0.0.1",
  "description": "api-gateway, test full stack company Watch",
  "author": "Luizpaim",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "nest build --config tsconfig.build.json",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npx dotenv-cli -e .env.development -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npx dotenv-cli -e .env -- node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npx dotenv-cli -e .env.test -- jest --runInBand",
    "test:unit": "npx dotenv-cli -e .env.test -- jest --runInBand --config ./jest.unit.config.ts",
    "test:int": "npx dotenv-cli -e .env.test -- jest --runInBand --config ./jest.int.config.ts",
    "test:watch": "jest --watch",
    "test:cov": "npx dotenv-cli -e .env.test -- jest --runInBand --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npx dotenv-cli -e .env -- jest --runInBand --config ./jest.e2e.config.ts",
    "prisma:client": "npx prisma generate --schema=./src/shared/infrastructure/database/prisma/schema.prisma",
    "prisma:migration:run": "npx prisma migrate dev --schema=./src/shared/infrastructure/database/prisma/schema.prisma",
    "docker:local": "docker-compose -f docker-compose.local.yml down -v --remove-orphans && docker-compose -f docker-compose.local.yml up --build"
  },
  "dependencies": {
    "@fastify/otel": "^0.9.0",
    "@fastify/static": "^8.2.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.2",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/platform-fastify": "^11.1.2",
    "@nestjs/platform-socket.io": "^11.1.2",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.60.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.202.0",
    "@opentelemetry/instrumentation-http": "^0.202.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/semantic-conventions": "^1.34.0",
    "@prisma/client": "^6.8.2",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^6.4.3",
    "cache-manager-ioredis-yet": "^2.1.2",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv-cli": "^8.0.0",
    "fastify-swagger": "^5.2.0",
    "ioredis": "^5.6.1",
    "kafkajs": "^2.2.4",
    "redis": "^5.5.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^5.0.2",
    "@types/hapi__shot": "^6.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.15.29",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.3"
  }
}
